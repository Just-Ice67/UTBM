{
  "extends": [
    "astro/tsconfigs/strict"
  ],
  "compilerOptions": {
    "noFallthroughCasesInSwitch": false,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@lib/*": ["src/lib/*"],
      "@pages/*": ["src/pages/*"]
    }
  },
  "include": [
    "src/**/*"
  ]
}